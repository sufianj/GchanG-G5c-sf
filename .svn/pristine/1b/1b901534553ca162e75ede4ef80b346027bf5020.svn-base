.resultatrech{
    background-color: #3366ff;
    width: 90%;
    text-align: left;
    padding-left: 10px;
    border-radius: 10px;
}
.resultatrech:hover{
    background-color: #6666cc;
}
.image{
    
    width:100px;
}
.resultatrech h1{
    margin : 0px;
}
article a{
    color: black;
    text-decoration: none;
}
.resultatrech a{
    text-decoration: underline;
}
