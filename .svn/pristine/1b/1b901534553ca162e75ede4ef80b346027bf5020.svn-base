*{
    font-family: Times New Roman;
    font-size: 16px;
}
#page{
    min-width: 800px;
    width: 90%;
    margin: auto;
    background-color: lightblue;
    border-radius: 10px;
}
a img{
    border: none;
}
article{
    margin-left: 10px;
    margin-top: 2%;
    min-width: 600px;
    width: 70%;
    display: inline-block;
    margin-right: -0.25em;
    vertical-align: top;

}

/*Listes Utilisateurs*/
#listeutilisateurs{
    border-collapse: collapse;
    
}
thead{
    text-align: center;
    font-weight: bold;
}
#listeutilisateurs td{
    border: solid black 1px;
    padding: 3px;  
}
#listeutilisateurs .image_suppression{
    width : 20px;
}
.croix{
    background-image: url(../../images/fauxpetit.png);
    background-repeat:no-repeat;
    background-position: top;
    height: 20px;
    cursor: pointer;
    font-size:0px;
    width: 20px;
    border: none;
    background-color: transparent;
}
.cadenasouv{
    background-image: url(../../images/cadenasouv.png);
